from tensorflow.keras.applications import DenseNet121
from tensorflow.keras.layers import Dense, ReLU, GlobalAveragePooling2D
from tensorflow.keras.models import Model

# Base model
base_model = DenseNet121(weights="imagenet", include_top=False, input_shape=(32,32,3), pooling="avg")
x = base_model.output
x = ReLU()(x)
output = Dense(10, activation='softmax')(x)
model = Model(inputs=base_model.input, outputs=output)

model.compile(optimizer='adam', loss='sparse_categorical_crossentropy', metrics=['accuracy'])
model.fit(x_train, y_train, validation_data=(x_val, y_val), epochs=25, batch_size=32)

print("Evaluation:", model.evaluate(x_test, y_test))
